{
  "name": "new-decorators-with-typescript",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@types/node": "^18.11.7",
    "@types/yargs-parser": "^21.0.0",
    "lodash": "^4.17.21",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "5.0.0-beta"
  },
  "scripts": {
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test/**",
      "**/docs/**",
      "node_modules"
    ],
    "verbose": true,
    "restartable": "rs",
    "execMap": {
      "ts": "ts-node-esm"
    },
    "delay": 500,
    "watch": [
      "*.ts"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "ts,json"
  },
  "dependencies": {
    "path-to-regexp": "^6.2.1",
    "yargs-parser": "^21.1.1"
  }
}
